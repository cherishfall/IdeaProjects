<mapper namespace="com.innotek.fragrans.dao.stat.ActivityDailyDao" > 
<!-- Result Map-->
<resultMap id="BaseResultMap" type="com.innotek.fragrans.entity.stat.ActivityDailyEntity" >
	<result column="id" property="id"/>
		<result column="day_time" property="dayTime"/>
		<result column="activity_count_login" property="activityCountLogin"/>
		<result column="activity_count_total" property="activityCountTotal"/>
		<result column="new_register_count" property="newRegisterCount"/>
		<result column="new_access_count" property="newAccessCount"/>
		<result column="next_day_still_use_count" property="nextDayStillUseCount"/>
		<result column="startup_count" property="startupCount"/>
		<result column="download_count_android" property="downloadCountAndroid"/>
		<result column="download_count_ios" property="downloadCountIos"/>
		<result column="download_count_all" property="downloadCountAll"/>
		<result column="create_time" property="createTime"/>
	</resultMap>
       
<!-- table all fields -->
<sql id="Base_Column_List" >
		 t.day_time,
		 t.activity_count_login,
		 t.activity_count_total,
		 t.new_register_count,
		 t.new_access_count,
		 t.next_day_still_use_count,
		 t.startup_count,
		 t.download_count_android,
		 t.download_count_ios,
		 t.download_count_all,
		 t.create_time,
	     t.id
</sql>

<!-- 查询条件 -->
<sql id="Example_Where_Clause">
	where 1=1
<trim  suffixOverrides="," >
	<if test="id != null" >
	    and t.id = #{id}
	</if>
	<if test="dayTime != null and dayTime != '' " >
    	and t.day_time =  #{dayTime}
	</if>
	<if test="activityCountLogin != null " >
    	and t.activity_count_login =  #{activityCountLogin}
	</if>
	<if test="activityCountTotal != null " >
    	and t.activity_count_total =  #{activityCountTotal}
	</if>
	<if test="newRegisterCount != null " >
    	and t.new_register_count =  #{newRegisterCount}
	</if>
	<if test="newAccessCount != null " >
    	and t.new_access_count =  #{newAccessCount}
	</if>
	<if test="nextDayStillUseCount != null " >
    	and t.next_day_still_use_count =  #{nextDayStillUseCount}
	</if>
	<if test="startupCount != null " >
    	and t.startup_count =  #{startupCount}
	</if>
	<if test="downloadCountAndroid != null " >
    	and t.download_count_android =  #{downloadCountAndroid}
	</if>
	<if test="downloadCountIos != null " >
    	and t.download_count_ios =  #{downloadCountIos}
	</if>
	<if test="downloadCountAll != null " >
    	and t.download_count_all =  #{downloadCountAll}
	</if>
	<if test="createTime != null " >
    	and t.create_time =  #{createTime}
	</if>
	</trim>
</sql>

<!-- 插入记录 -->
