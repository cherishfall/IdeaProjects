<select id="queryChargeRecordById"  resultMap="BaseResultMap" parameterType="Object">
	select <include refid="ChargeRecord_Column_List" /> 
	 from APP_CHARGE_RECORD t 
	 left join base_area ba on t.city_code = ba.area_code
	 left join app_user au on t.user_id = au.id
	 where t.id = #{id}
		 AND ROWNUM = 1
</select>

