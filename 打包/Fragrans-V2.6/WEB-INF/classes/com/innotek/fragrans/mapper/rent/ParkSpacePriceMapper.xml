			#{item.remark},
			#{item.flag},
			#{item.createdBy},
			#{item.createTime}
		 )  
	</foreach> 
	</insert>
<!--根据泊位ID查询计费信息 -->
<select id="queryByBerthId" resultMap="BaseResultMap"  parameterType="java.lang.String">
	SELECT * FROM rent_park_space_price
	WHERE flag != 'DELETE'
	  AND berth_id = #{_parameter}
</select>
