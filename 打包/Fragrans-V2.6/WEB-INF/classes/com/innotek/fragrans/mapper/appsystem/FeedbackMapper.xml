<!-- 删除记录 -->
<delete id="delete" parameterType="Object">
	delete from APP_FEEDBACK where id = #{id}
</delete>
 
<!-- 根据id查询 -->
<select id="queryById"  resultMap="BaseResultMap" parameterType="Object">
	select <include refid="Base_Column_List" /> 
	 from APP_FEEDBACK  t where t.id = #{id}
</select>

<!-- 列表总数-->
<select id="queryByCount" resultType="java.lang.Integer"  parameterType="Object">
	select count(1) from APP_FEEDBACK  t 
	<include refid="Example_Where_Clause"/>
	<if test="userCityCode != null and userCityCode != '' ">
		and t.CITY_CODE LIKE '${userCityCode}%'
	</if>
</select>
  	
<!-- 查询列表 -->
<select id="queryByList" resultMap="BaseResultMap"  parameterType="Object">
	select 
	<include refid="Base_Column_List"/>
	,ba.area_name
	from APP_FEEDBACK t 
	left join base_area ba on t.city_code = ba.area_code
	<include refid="Example_Where_Clause"/>
	<if test="userCityCode != null and userCityCode != '' ">
		and t.CITY_CODE LIKE '${userCityCode}%'
	</if>
	<if test="pager.orderCondition == ''">
		order by t.create_time desc
	</if>
	<if test="pager.orderCondition != null and pager.orderCondition != ''" >
      ${pager.orderCondition}
    </if>
     <if test="pager.mysqlQueryCondition != null and pager.mysqlQueryCondition != ''" >
       ${pager.mysqlQueryCondition}
    </if>
</select>
<!-- *************************************自定义接口****************************************************** -->
<!-- 根据id，修改记录-->  
<update id="ProcessingFeedback" parameterType="java.util.Map" >
	update APP_FEEDBACK  t set 
   <trim suffixOverrides="," >
    <if test="processResult != null and processResult != '' " >
  	<![CDATA[  t.process_result = #{processResult}, ]]>
    </if>
    <if test="processedBy != null and processedBy != '' " >
  	<![CDATA[  t.processed_by = #{processedBy}, ]]>
    </if>
  	<![CDATA[  t.process_time = now(), ]]>
  	<![CDATA[  t.update_time = now(), ]]>
    <if test="updatedBy != null and updatedBy != '' " >
  	<![CDATA[  t.updated_by = #{updatedBy}, ]]>
    </if>
     </trim> where t.id= #{id}
 </update>
 
  <!-- 查询所有记录 -->
 <select id="queryAllFeedback" resultMap="BaseResultMap" parameterType="java.lang.String">
	select 
	<include refid="Base_Column_List"/>
	,ba.area_name from APP_FEEDBACK t
	left join base_area ba on t.city_code = ba.area_code
	where 1=1
	<if test="_parameter != null and _parameter != '' ">
		and t.CITY_CODE LIKE '${_parameter}%'
	</if>
	order by t.city_code asc
 </select>
   <!-- 查询所有记录 -->
 <select id="queryFeedbackByUser" resultMap="BaseResultMap" parameterType="java.lang.String">
	select 
	<include refid="Base_Column_List"/>
	 from APP_FEEDBACK t
	where 1=1
	<if test="_parameter != null and _parameter != '' ">
		and t.ACCOUNT = '${_parameter}%'
	</if>
	order by t.update_time desc
 </select>
</mapper>   
