<mapper namespace="com.innotek.fragrans.dao.appsystem.FeedbackDao" > 
<!-- Result Map-->
<resultMap id="BaseResultMap" type="com.innotek.fragrans.entity.appsystem.FeedbackEntity" >
	<result column="id" property="id"/>
		<result column="city_code" property="cityCode"/>
		<result column="device_id" property="deviceId"/>
		<result column="account" property="account"/>
		<result column="plate" property="plate"/>
		<result column="phone" property="phone"/>
		<result column="contact" property="contact"/>
		<result column="type" property="type"/>
		<result column="content" property="content"/>
		<result column="app_id" property="appId"/>
		<result column="device_type" property="deviceType"></result>
		<result column="app_version" property="appVersion"></result>
		<result column="machine_type" property="machineType"></result>
		<result column="process_status" property="processStatus"/>
		<result column="process_result" property="processResult"/>
		<result column="processed_by" property="processedBy"/>
		<result column="process_time" property="processTime"/>
		<result column="flag" property="flag"/>
		<result column="create_time" property="createTime"/>
		<result column="created_by" property="createdBy"/>
		<result column="update_time" property="updateTime"/>
		<result column="updated_by" property="updatedBy"/>
		<result column="area_name" property="areaName"/>
	</resultMap>
       
<!-- table all fields -->
<sql id="Base_Column_List" >
		 t.city_code,
		 t.device_id,
		 t.account,
		 t.plate,
		 t.contact,
		 t.phone,
		 t.type,
		 t.content,
		 t.app_id,
		 t.device_type,
		 t.machine_type,
		 t.app_version,
		 t.process_status,
		 t.process_result,
		 t.processed_by,
		 t.process_time,
		 t.flag,
		 t.create_time,
		 t.created_by,
		 t.update_time,
		 t.updated_by,
	     t.id
</sql>

<!-- 查询条件 -->
<sql id="Example_Where_Clause">
	where 1=1
<trim  suffixOverrides="," >
	<if test="id != null" >
	    and t.id = #{id}
	</if>
	<if test="cityCode != null" >
	    and t.cioty_code = #{cityCode}
	</if>
	<if test="deviceId != null and deviceId != '' " >
    	and t.device_id =  #{deviceId}
	</if>
	<if test="deviceType != null and deviceType != '' " >
    	and t.device_type = #{deviceType}
	</if>
	<if test="machineType != null and machineType != '' " >
    	and t.machine_type = #{machineType}
	</if>
	<if test="appVersion != null and appVersion != '' " >
    	and t.app_version =  #{appVersion}
	</if>
	<if test="account != null and account != '' " >
    	and t.account =  #{account}
	</if>
	<if test="plate != null and plate != '' " >
    	and t.plate like '%${plate}%'
	</if>
	<if test="contact != null and contact != '' " >
    	and t.contact =  #{contact}
	</if>
	<if test="phone != null and phone != '' " >
    	and t.phone =  #{phone}
	</if>
	<if test="type != null " >
    	and t.type =  #{type}
	</if>
	<if test="content != null and content != '' " >
    	and t.content like '%${content}%'
	</if>
	<if test="appId != null and appId != '' " >
    	and t.app_id =  #{appId}
	</if>
	<if test="processStatus != null" >
		and t.process_status =  #{processStatus}
	</if>
	<if test="processResult != null and processResult != '' " >
	    <if test="processResult=='yes'">
    	  and t.process_result is not null
	    </if>
	     <if test="processResult=='no'">
    	  and t.process_result is null
	    </if>
	</if>
	<if test="processedBy != null and processedBy != '' " >
    	and t.processed_by =  #{processedBy}
	</if>
	<if test="processTime != null " >
    	and t.process_time =  #{processTime}
	</if>
	<if test="flag != null and flag != '' " >
    	and t.flag =  #{flag}
	</if>
	<if test="createTime != null " >
    	and t.create_time =  #{createTime}
	</if>
	<if test="createdBy != null and createdBy != '' " >
    	and t.created_by =  #{createdBy}
	</if>
	<if test="updateTime != null " >
    	and t.update_time =  #{updateTime}
	</if>
	<if test="updatedBy != null and updatedBy != '' " >
    	and t.updated_by =  #{updatedBy}
	</if>
	<if test="startTime != null and startTime != '' and endTime != null and endTime != '' "  >
    	and t.create_time between  #{startTime} and  #{endTime}
	</if>
	</trim>
</sql>

<!-- 插入记录 -->
<insert id="saveFeedback" parameterType="Object" >
  
	<selectKey resultType="java.lang.String" order="BEFORE"
			keyProperty="id">
			SELECT UUID() as ID
	</selectKey>
  
	insert into APP_FEEDBACK
	  <trim prefix="(" suffix=")" suffixOverrides="," >
	     flag,
	    <if test="id != null">
				id,
		</if>
	    <if test="cityCode != null">
				city_code,
		</if>
	 	<if test="deviceId != null and deviceId != '' " >
		 device_id,
		</if>
	 	<if test="deviceType != null and deviceType != '' " >
		 device_type,
		</if>
	 	<if test="machineType != null and machineType != '' " >
		 machine_type,
		</if>
	 	<if test="appVersion != null and appVersion != '' " >
		 app_version,
		</if>
    	<if test="account != null and account != '' " >
		 account,
		</if>
    	<if test="plate != null and plate != '' " >
		 plate,
		</if>
    	<if test="contact != null and contact != '' " >
		 contact,
		</if>
    	<if test="phone != null and phone != '' " >
		 phone,
		</if>
    	<if test="type != null " >
		type,
		</if>
    	<if test="content != null and content != '' " >
		 content,
		</if>
    	<if test="appId != null and appId != '' " >
		 app_id,
		</if>
    	<if test="processResult != null and processResult != '' " >
		 process_result,
		</if>
    	<if test="processedBy != null and processedBy != '' " >
		 processed_by,
		</if>
    	<if test="processTime != null " >
		process_time,
		</if>
