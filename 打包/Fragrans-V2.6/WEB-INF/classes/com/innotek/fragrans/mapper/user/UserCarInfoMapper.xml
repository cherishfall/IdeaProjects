    </if>
    <if test="updatedBy != null and updatedBy != '' " >
  	<![CDATA[  t.updated_by = #{updatedBy}, ]]>
    </if>
    <if test="updateTime != null " >
  	<![CDATA[  t.update_time = #{updateTime}, ]]>
    </if>
     </trim> where t.id= #{id}
 </update>

<!-- 删除记录 -->
<delete id="delete" parameterType="Object">
	delete from app_user_car_info where id = #{id}
</delete>
 
<!-- 根据id查询 -->
<select id="queryById"  resultMap="BaseResultMap" parameterType="Object">
	select <include refid="Base_Column_List" /> 
	 from app_user_car_info t where t.id = #{id}
</select>

<!-- 列表总数-->
<select id="queryByCount" resultType="java.lang.Integer"  parameterType="Object">
	select count(1) from app_user_car_info t 
	<include refid="Example_Where_Clause"/>
</select>
  	
<!-- 查询列表 -->
<select id="queryByList" resultMap="BaseResultMap"  parameterType="Object">
	select 
	<include refid="Base_Column_List"/>
	from app_user_car_info t 
	<include refid="Example_Where_Clause"/>
	<if test="pager.orderCondition == ''">
		order by t.id desc
	</if>
	<if test="pager.orderCondition != null and pager.orderCondition != ''" >
      ${pager.orderCondition}
    </if>
     <if test="pager.mysqlQueryCondition != null and pager.mysqlQueryCondition != ''" >
       ${pager.mysqlQueryCondition}
    </if>
</select>
<!-- *************************************自定义接口****************************************************** -->
	<!-- 根据plateId查询是否存在正在审核中的日志 -->
	<select id="queryIsExistReviewingLog" resultType="java.lang.Integer" parameterType="Object">
		SELECT COUNT(1) FROM app_user_car_info a
		LEFT JOIN app_review_log b ON a.id = b.busi_id
		WHERE a.car_id = #{plateId} and b.REVIEW_STATUS != 2 and b.REVIEW_STATUS != 3 
	</select>
	
	<!-- 根据plateId查询是否存在已审核通过的日志 -->
	<select id="queryIsExistHasReviewLog" resultType="java.lang.Integer" parameterType="Object">
		SELECT COUNT(1) FROM app_user_car_info a
		LEFT JOIN app_review_log b ON a.id = b.busi_id
		WHERE a.car_id = #{plateId} and b.REVIEW_STATUS = 2
	</select>
	
	<!-- 根据car_id查询(未审核和审核中的) -->
	<select id="queryReviewingByCarId"  resultMap="BaseResultMap" parameterType="Object">
		SELECT t.* FROM app_user_car_info t
		LEFT JOIN app_review_log r ON r.busi_id = t.id
		WHERE t.car_id = #{carId} and (r.review_status = 0 or r.review_status = 1) 
	</select>
	
	<!-- 根据id批量删除记录 -->
	<delete id="deleteByIds" parameterType="Object">
		delete from app_user_car_info where id in 
		<foreach collection="list" item="id" open="(" close=")" separator=",">   
            #{id}  
        </foreach> 
	</delete>
</mapper>   
