<!-- 根据id，修改记录-->  
<update id="update" parameterType="Object" >
	update app_user_car_info t set 
   <trim suffixOverrides="," >
	<if test="carId != null and carId != '' " >
  	<![CDATA[  t.car_id = #{carId}, ]]>
    </if>
    <if test="carBrand != null and carBrand != '' " >
  	<![CDATA[  t.car_brand = #{carBrand}, ]]>
    </if>
    <if test="carColor != null and carColor != '' " >
  	<![CDATA[  t.car_color = #{carColor}, ]]>
    </if>
    <if test="vehicleType != null and vehicleType != '' " >
  	<![CDATA[  t.vehicle_type = #{vehicleType}, ]]>
    </if>
    <if test="vehicleNumber != null and vehicleNumber != '' " >
  	<![CDATA[  t.vehicle_number = #{vehicleNumber}, ]]>
    </if>
    <if test="engineNumber != null and engineNumber != '' " >
  	<![CDATA[  t.engine_number = #{engineNumber}, ]]>
    </if>
