<!-- 根据id查询 -->
<select id="queryById"  resultMap="BaseResultMap" parameterType="Object">
	select <include refid="Base_Column_List" /> 
	 from rent_park_space_info t where t.id = #{id}
</select>

<!-- 列表总数-->
<select id="queryByCount" resultType="java.lang.Integer"  parameterType="Object">
	select count(1) from rent_park_space_info t 
	where 1=1
	<if test="isLimit == 1 " >
    	and t.status = 1
	</if>
	<if test="type != null " >
    	and t.type =  #{type}
	</if>
	<if test="status != null " >
    	and t.status =  #{status}
	</if>
	<if test="createdBy != null and createdBy != '' " >
    	and t.created_by like '${createdBy}%'
	</if>
</select>
  	
<!-- 查询列表 -->
<select id="queryByList" resultMap="BaseResultMap"  parameterType="Object">
	select 
	<include refid="Base_Column_List"/>
	,r.reviewer,r.review_result
	from rent_park_space_info t 
	left join rent_park_space_review r on t.id = r.berth_id
	where 1=1
	<if test="isLimit == 1 " >
    	and t.status = 1
	</if>
	<if test="type != null " >
    	and t.type =  #{type}
	</if>
	<if test="status != null " >
    	and t.status =  #{status}
	</if>
	<if test="createdBy != null and createdBy != '' " >
    	and t.created_by like '${createdBy}%'
	</if>
	<if test="pager.orderCondition == ''">
		order by t.status asc,t.create_time asc
	</if>
	<if test="pager.orderCondition != null and pager.orderCondition != ''" >
      ${pager.orderCondition}
    </if>
     <if test="pager.mysqlQueryCondition != null and pager.mysqlQueryCondition != ''" >
       ${pager.mysqlQueryCondition}
    </if>
</select>
