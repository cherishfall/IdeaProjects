</resultMap>

<!--根据经纬度查询发布车位 -->
<select id="getMapShareParkSpaceRecord" resultMap="MapRecordMap"  parameterType="Object">
	SELECT id,type,longitude_gaode,latitude_gaode,address
	FROM rent_park_space_info
	WHERE STATUS = 2 AND FLAG != 'DELETE'
	  AND LONGITUDE_GAODE &gt;= #{minLongitude} AND LONGITUDE_GAODE &lt;= #{maxLongitude}
	  AND LATITUDE_GAODE &gt;= #{minLatitude} AND LATITUDE_GAODE &lt;= #{maxLatitude}
</select>


<!--根据泊位ID更新评价数 -->
<update id="updateEvalCountByBerthId" parameterType="java.lang.String">
	UPDATE rent_park_space_info
	SET EVAL_NUM = EVAL_NUM + 1
	WHERE ID = #{_parameter}
</update>

<resultMap id="ParkSpaceRecordMap" type="com.innotek.fragrans.bean.item.shareparkspace.ShareParkSpaceRecordItem" >
	<result column="id" property="berthId"/>
	<result column="longitude_gaode" property="longitudeGD"/>
	<result column="latitude_gaode" property="latitudeGD"/>
	<result column="name" property="parkName"/>
	<result column="address" property="address"/>
	<result column="status" property="status"/>
	<result column="img_url" property="imgUrl"/>
	<result column="map_address" property="mapAddress"/>
</resultMap>
<!--根据状态查询车位发布信息-->
<select id="queryParkSpaceInfoByStatus" resultMap="ParkSpaceRecordMap"  parameterType="Object">
	SELECT id,longitude_gaode,latitude_gaode,name,address,status,img_url,map_address,
	round(6378.138*2*asin(sqrt(pow(sin((#{latitude}*pi()/180-latitude_gaode*pi()/180)/2),2)+cos(#{latitude}*pi()/180)*cos(latitude_gaode*pi()/180)* pow(sin((#{longitude}*pi()/180-longitude_gaode*pi()/180)/2),2))),2)
	as distance
	FROM rent_park_space_info
	WHERE STATUS = #{status} AND FLAG != 'DELETE'
	order by distance asc
	limit #{pageIndex},#{pageLength}
</select>

<!-- 根据用户查询车位发布信息 -->
<select id="queryParkSpaceInfoByUserId" resultMap="ParkSpaceRecordMap"  parameterType="Object">
	SELECT id,longitude_gaode,latitude_gaode,name,address,status,img_url,map_address
	FROM rent_park_space_info
	WHERE user_id = #{userId} AND FLAG != 'DELETE'
	order by create_time desc
	limit #{pageIndex},#{pageLength}
</select>
